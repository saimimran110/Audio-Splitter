{
  "builds": [
    {
      "src": "audio-splice-studio/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/audio-splice-studio/$1"
    }
  ],
  "outputDirectory": "audio-splice-studio/dist"
}